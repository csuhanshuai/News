<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="#F7F7F7"
        android:paddingLeft="10dp" >

        <RelativeLayout
            android:id="@+id/go_back6"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:background="#F7F7F7" >

            <ImageView
                android:id="@+id/back3"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_alignParentLeft="true"
                android:layout_centerInParent="true"
                android:src="@drawable/arrow_left" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_toRightOf="@+id/back3"
                android:text="我"
                android:textColor="#3384FF"
                android:textSize="18sp" />
        </RelativeLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="保险顾问个人信息"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/complete1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerInParent="true"
            android:layout_marginRight="10dp"
            android:text="修改"
            android:textColor="#3384FF"
            android:textSize="18sp" />
    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/tupian"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/corner_view"
                android:clickable="true"
                android:gravity="left|center" >

                <net.bangbao.widget.CircleImage
                    android:id="@+id/photo"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_gravity="left|center"
                    android:background="@drawable/consult_default" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:gravity="center"
                    android:text="@string/username"
                    android:textColor="#666666"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/username"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="23dp"
                    android:gravity="center|left"
                    android:textSize="16sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:focusable="true"
                android:focusableInTouchMode="true" />

            <LinearLayout
                android:id="@+id/pw"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/corner_view"
                android:gravity="left|center"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="原始密码"
                        android:textColor="#666666"
                        android:textSize="16sp" />

                    <EditText
                        android:id="@+id/old_pwd"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="5dp"
                        android:background="@color/login_edittext_background_blue"
                        android:gravity="bottom"
                        android:hint="********"
                        android:maxLength="16" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/new_password"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginBottom="15dp"
                    android:layout_weight="1" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="修改密码"
                        android:textColor="#666666"
                        android:textSize="16sp" />

                    <EditText
                        android:id="@+id/new_pwd"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="20dp"
                        android:background="@color/login_edittext_background_blue"
                        android:textSize="16sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/repw"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginBottom="5dp"
                    android:layout_weight="1" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="@string/confirm"
                        android:textColor="#666666"
                        android:textSize="16sp" />

                    <EditText
                        android:id="@+id/confirm"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="5dp"
                        android:background="@color/login_edittext_background_blue"
                        android:textSize="16sp" />
                </LinearLayout>
            </LinearLayout>

            <TextView
                android:id="@+id/toast"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="3dp"
                android:visibility="gone" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="3dp"
                android:background="@drawable/corner_view"
                android:gravity="left|center"
                android:orientation="vertical" 
                android:visibility="gone">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="@string/english_name"
                        android:textColor="#666666"
                        android:textSize="16sp" />

                    <EditText
                        android:id="@+id/english_name"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="20dp"
                        android:background="@color/login_edittext_background_blue"
                        android:maxLength="80"
                        android:textColor="#666666"
                        android:textSize="16sp" />
                </LinearLayout>

                <!-- 性别 -->

                <RelativeLayout
                    android:id="@+id/sex"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginBottom="15dp"
                    android:layout_weight="1" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_marginTop="3dp"
                        android:gravity="center"
                        android:text="@string/sex"
                        android:textColor="#666666"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/sex1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_marginRight="10dp"
                        android:background="@color/login_edittext_background_blue"
                        android:gravity="center"
                        android:text="男"
                        android:textColor="#666666"
                        android:textSize="16sp" />

                    <ImageView
                        android:id="@+id/sex_arrow"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="8dp"
                        android:src="@drawable/bt_down" />
                </RelativeLayout>
                <!-- 公司 -->

                <RelativeLayout
                    android:id="@+id/company"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginBottom="15dp"
                    android:layout_weight="1" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_marginTop="3dp"
                        android:gravity="left"
                        android:text="@string/company"
                        android:textColor="#666666"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/com"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_marginRight="10dp"
                        android:background="@color/login_edittext_background_blue"
                        android:gravity="center"
                        android:text="英国保诚"
                        android:textColor="#666666"
                        android:textSize="16sp" />

                    <ImageView
                        android:id="@+id/comp_arrow"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="8dp"
                        android:src="@drawable/bt_down" />
                </RelativeLayout>
                <!-- 联系方式 -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginBottom="10dp"
                    android:layout_weight="1" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="@string/contact"
                        android:textColor="#666666"
                        android:textSize="16sp" />

                    <EditText
                        android:id="@+id/contact"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="5dp"
                        android:background="@color/login_edittext_background_blue"
                        android:maxLength="30"
                        android:textColor="#666666"
                        android:textSize="16sp" />
                </LinearLayout>
                <!-- 籍贯 -->

                <RelativeLayout
                    android:id="@+id/ori"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginBottom="10dp"
                    android:layout_weight="1" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="3dp"
                        android:gravity="center"
                        android:text="@string/origin"
                        android:textColor="#666666"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/origin"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_marginRight="10dp"
                        android:background="@color/login_edittext_background_blue"
                        android:gravity="center"
                        android:text="上海"
                        android:textColor="#666666"
                        android:textSize="16sp" />

                    <ImageView
                        android:id="@+id/origin_arrow"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="8dp"
                        android:src="@drawable/bt_down" />
                </RelativeLayout>
                <!-- 出生年月日 -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginBottom="10dp"
                    android:layout_weight="1" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="@string/birthday"
                        android:textColor="#666666"
                        android:textSize="16sp" />

                    <EditText
                        android:id="@+id/year1"
                        android:layout_width="40dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:background="@color/login_edittext_background_blue"
                        android:numeric="integer"
                        android:text="19"
                        android:maxLength="4"
                        android:digits="1234567890"
                        android:textColor="#666666"
                        android:textSize="16sp" />

                    <TextView
                        android:layout_width="20dp"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text="年"
                        android:textColor="#666666"
                        android:textSize="16sp" />

                    <EditText
                        android:id="@+id/month1"
                        android:layout_width="20dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="5dp"
                        android:background="@color/login_edittext_background_blue"
                        android:numeric="integer"
                        android:maxLength="2"
                        android:digits="1234567890"
                        android:textColor="#666666"
                        android:textSize="16sp" />

                    <TextView
                        android:layout_width="20dp"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text="月"
                        android:textColor="#666666"
                        android:textSize="16sp" />

                    <EditText
                        android:id="@+id/day1"
                        android:layout_width="20dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="5dp"
                        android:background="@color/login_edittext_background_blue"
                        android:numeric="integer"
                        android:digits="1234567890"
                        android:maxLength="2"
                        android:textColor="#666666"
                        android:textSize="16sp" />

                    <TextView
                        android:layout_width="20dp"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text="日"
                        android:textColor="#666666"
                        android:textSize="16sp" />
                </LinearLayout>
                <!-- 星座 -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginBottom="10dp"
                    android:layout_weight="1" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="@string/constellation"
                        android:textColor="#666666"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/constellation"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="35dp"
                        android:gravity="center|left"
                        android:textColor="#666666"
                        android:textSize="16sp" />
                </LinearLayout>
            </LinearLayout>

